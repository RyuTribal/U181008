<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Cinzel" rel="stylesheet">
    <link href="~/Content/rest.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Arapey" rel="stylesheet">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" id="fade">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("W.E.I", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("About", "About", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Contact", "Contact", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <footer>
            <ul>
                <li><a href="https://ryutribal.netlify.com/">Ivan</a></li>
                <li><a href="https://ryutribal.netlify.com/">Erik</a></li>
                <li><a href="https://ryutribal.netlify.com/">Wilhelm</a></li>
            </ul>
            <p>&copy; @DateTime.Now.Year - Team W.E.I</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>